<?xml version='1.0' encoding='utf-8'?>
<widget id="com.titans.yung" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <feature name="NativePageTransitions">
        <param name="android-package" value="com.telerik.plugins.nativepagetransitions.NativePageTransitions" />
        <param name="onload" value="true" />
    </feature>
    <splash density="land-hdpi" src="res/screen/android/1.png" />
    <splash density="land-ldpi" src="res/screen/android/1.png" />
    <splash density="land-mdpi" src="res/screen/android/1.png" />
    <splash density="land-xhdpi" src="res/screen/android/1.png" />
    <splash density="port-hdpi" src="res/screen/android/1.png" />
    <splash density="port-ldpi" src="res/screen/android/1.png" />
    <splash density="port-mdpi" src="res/screen/android/1.png" />
    <splash density="port-xhdpi" src="res/screen/android/1.png" />
    <allow-intent href="market:*" />
    <name>Yung</name>
    <description>
       Yung - You are never lost.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        A project by Team Titans.
    </author>
    <content src="view/home.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashScreenDelay" value="8000" />
    <preference name="ShowSplashScreenSpinner" value="true" />
</widget>
